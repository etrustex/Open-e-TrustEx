}



/************************************************/
/********** CONTENT BOXES ***********************/
/************************************************/

.box-wrapper 								{ width: auto; background: transparent; border: 1px solid #cacaca; margin:0 25px 20px 0; }
.box .box-wrapper                           { margin-right: 5px; }
.ie7 .box-wrapper                           { margin-bottom: 15px !important; }
.box-wrapper.highlight                      { border: 1px solid #689be6;}
.bw .box-wrapper.highlight                  { border: 1px solid #8D8D8D;}
.box > .header                              { margin: 0; padding: 0; height: 42px; position: relative;
                                              background: #efefef url(data:image/png;base64,) repeat-x; }
.ie7 .box > .header                         { background-image: url(images/_base64_sources/bg_box_header.png);}
.box > .header h2 							{ padding: 8px 0 0 12px;float: left;width: auto; color: #3A3A3A;}
.box-wrapper.highlight > .box > .header h2  { color: #007abd;}
.box > .header.no-icon h2                   { padding-left: 16px;}
.box.alt > .header h2                       { font-size: 18px; color: #666; padding-left:16px; }

.box.red > .header h2						{ color: #FF0000; }
.bw .box.red > .header h2					{ color: #000 ; }
.box.green > .header h2 					{ color: #11991B; }
.bw .box.green > .header h2 				{ color: #666 ; }
.box > .header > .icon  					{ display: none; /*display: inline; float: left; height:16px; width: 16px; text-indent:-9999px; margin:12px 0 0 10px;	*/}
.box > .header > .header-left   			{ float: left; position: relative; top: 2px;}
.box > .header > .header-right  			{ float: right; position: relative; top: 4px; margin-right: 0; }
.box > .header > .header-right > a          { float: right; position: relative; top: 7px; margin-left: 5px; right:10px;}
.box > .header > .header-right > a.button-link { top: 4px; right:0;}
.box > .header > .header-right > .field      { margin-right: 10px;}
.box > .header.JS_expandable					{ cursor: pointer; }
.box > .header.JS_expandable > .expand-button 	{ float:right; margin-top: 10px; text-indent: -9999px; width: 26px; height: 20px; cursor: pointer; }
.box > .header.JS_expandable > .header-right 	{ margin-right: 30px !important; }
.box > .content 							{ padding: 12px 12px 12px 12px; background: #FFF ; border-top: 1px solid #D4D5D5;}
.bw .box > .content 					    { border-top: 1px solid #bfbfbf;}
.box-wrapper.no-header .box > .content      { padding-top: 24px; }
.box > .content.grid                        { background: #dedddd; }
.box > .content.grid-grey                   { background: #f2F2F2;}
.box > .content.dark                        { background: #828080; }
.bw .box > .content.dark                    { background-color: #dedddd; }
.box > .content.bg                          { background: #ffffff url('images/backgrounds/bg_body_structure.png') repeat; }
.box > .content.bg-stripes                  { background: #ffffff url('images/backgrounds/bg_body_3.gif') repeat; padding:5px;}
.box > .content.releases,
.box > .content.list-container              { padding:5px; background-color:#ebebeb;}
.box > .content.news                        { padding:10px 5px 5px 15px; background-color:#fff;}
.box > .content.no-pad                      { padding: 0px;}
.box.red > .content							{ background-color: #FAE3E4; color: #FF0000; }
.bw .box.red > .content						{ background-color: #fff;  color: #000 ; }
.box.green > .content						{ background-color: #E3FAE5; color: #11991B; }
.bw .box.green > .content					{ background-color: #fff ; color: #000 ;}
.box.blue > .content                        { background-color: #e3edfa; color: #007abd; }
.bw .box.blue > .content                    { background-color: #fff ; color: #000 ;}


/*  BOX HEADER WITH TABS  */
.box > .header  ul 					   		{ float:left; position: absolute; bottom: 1px; list-style:none; top:13px; left:5px;}
.box > .header  ul li 						{ float:left; background:url(images/backgrounds/bg_tab_inactive_left.gif) no-repeat top left; padding-left:12px; height:34px; }
.box > .header  ul li.warning			   	{ float:left; background:url(images/backgrounds/bg_tab_inactive_left.gif) no-repeat top left; padding-left:12px; height:34px; }
.box > .header  ul li.selected 		   		{ background-image:url(images/backgrounds/bg_tab_active_left.gif); 	position:relative; }
.box > .header  ul li.warning.selected a  	{ color:#FF0000 !important; }
.bw .box > .header  ul li.warning.selected a { color:#000 ; }
.box > .header  ul li a 				   	{ color:#858585; text-decoration:none; display:block; padding:7px 15px 12px 12px; background:url(images/backgrounds/bg_tab_inactive_right.gif) no-repeat top right; font-weight: normal !important;}
.box > .header  ul li.selected a 		   	{ background-image:url(images/backgrounds/bg_tab_active_right.gif) !important;  color:#007abd; }
.ie .box > .header  ul li a                 { padding: 5px 25px 9px 12px; }
.bw .box > .header  ul li.selected a 	    { color:#333; }
.box > .header  ul li.warning a 		   	{ color:#dc4f4f; 	}
.bw .box > .header  ul li.warning a 	    { color:#000; }
.box > .header  ul li.with-notification a 	{ padding: 5px 0 15px 12px; }
.ie .box > .header  ul li.with-notification a { padding: 5px 32px 9px 12px; }

.box > .header ul.alt                       { top: 0; left:0;}
.box > .header ul.alt li					{ padding-left:10px; padding-right: 10px; float:left; height:42px; background: #F0F0F0;  border-right: 1px solid #dedede; border-bottom: 1px solid #E2E2E2;}
.ie .box > .header ul.alt li				{ padding-left:10px; padding-right: 0; }
.box > .header ul.alt li a 				   	{ color:#858585; text-align:center; text-decoration:none; display:block; padding:7px 0 10px 0; background:transparent; margin:5px 10px 0 10px; font-weight: normal !important; text-transform: uppercase;}
.box > .header ul.alt li.selected 	  		{ background-image: none; position:relative; background: #fff; border-bottom: 1px solid #fff; }
.box > .header ul.alt li.selected a 	  	{ background-image: none !important; color:#007abd; border-bottom:1px solid #007abd;}

ul.sub-tabs                 { display: inline-block; margin-bottom: 10px; padding-left: 10px; width: 100%; border-bottom: 1px solid #E2E2E2;}
ul.sub-tabs li              { float:left; padding-left:10px; padding-right: 10px; float:left; height:25px; }
ul.sub-tabs li a   	        { color:#858585; text-decoration:none; display:block; padding:1px 0 6px 0; background:transparent; margin:0 10px 0 10px; font-weight: normal !important; text-transform: uppercase;}
ul.sub-tabs li.selected 	{ background-image: none; position:relative; border-bottom: 1px solid #fff; }
ul.sub-tabs li.selected a 	{ background-image: none !important; color:rgb(60, 181, 60); border-bottom:1px solid rgb(60, 181, 60); font-size: 14px;}
.sub-tabs-content-wrapper   { display: block; padding: 10px 10px 20px 20px; }

.box > .header  ul li.selected a:hover    	{ color:#007abd; }
.bw .box > .header  ul li.selected a:hover  { color:#333; }
.box > .header  ul li a:hover 				{ color:#fff; }

.box > .header  ul.alt li a:hover			{ color:#007abd; }

.box > .header  ul li .button-notification 		         	{ color: #ffffff; text-shadow: 1px 0 0 #333333; border: 0 none; position: relative;	float:right; width: 20px;	height: 20px; margin-left: 5px; margin-top: -26px; padding-top: 2px; display: inline; text-align: center;	}
.ie .box > .header  ul li .button-notification 		        { position: absolute !important; margin-top:-9px !important;}
.ff .box > .header  ul li .button-notification         { margin-top: -12px; }
.box > .header  ul.alt li .button-notification 		       	{ margin-top: -36px; margin-right:-15px; margin-left:-2px;}

.box > .header  ul li .button-notification.with-items      	{ background: transparent url('images/backgrounds/bg_notification_small.png') no-repeat center; }
.box > .header  ul li .button-notification.with-items-info 	{ background: transparent url('images/backgrounds/bg_notification_blue_small.png') no-repeat center; }
.box > .header  ul li .button-notification.without-items   	{ background: transparent url('images/backgrounds/bg_notification_grey_small.png') no-repeat center !important; }

.box > .header  ul li .multiple-button-notification        	{  color: #ffffFf; border: 1px solid #bfbebe; position: relative;	float:right; width: 20px;	margin-left: 5px;  margin-right: 8px; margin-top: -26px;  display: inline-block; text-align: center; }
.ie .box > .header  ul li .multiple-button-notification     {  position: absolute;	margin-top: -12px; }
.ff .box > .header  ul li .multiple-button-notification {  margin-top: -12px; }
.box > .header  ul.alt li .multiple-button-notification     {  margin-left: 22px;  margin-right: -18px;  }
.ie .box > .header ul.alt li .multiple-button-notification  {  margin-left: 0; top: 15px; }
