/*********************************************************/
/********** GLOBAL ERROR DIALOG AND MESSAGES**************/
/*********************************************************/

#globalErrorDialog              {  border: 2px solid #ff0000 !important; }
#globalErrorDialog .title   	{  background:url(images/msgbox/background-error.png) repeat-x left top; color: #FF0000; padding: 10px 0 20px 0; margin-bottom: 10px; text-align:center; width:100%;}
#error-content      	        {  background-color: #f2f2f2; padding:10px; margin-left:0;}
#error-content .title   	    {  background:url(images/msgbox/background-error.png) repeat-x left top; color: #FF0000; padding: 10px 0 20px 0; margin-bottom: 10px; text-align:center; width:100%;}


#top_message                      { position: fixed; z-index:20000; width: 100%; }
.ie7 #top_message                 { position: absolute !important; }
#top_message .message             { color: #fff; padding: 10px; border-bottom: 2px solid;}
#top_message .message.info        { background-color: #4ea5cd; border-color: #1b83b5; }
#top_message .message.error       { background-color: #de4343; border-color: #c41d1d; }
#top_message .message.warning     { background-color: #eaaf51; border-color: #d99221; }
#top_message .message.success     { background-color: #61b832; border-color: #47a118; }
#top_message .message h3          { color: #fff; margin: 0 0 5px 0; font-size: 16px; }
#top_message .message p           { margin-bottom: 10px; }
#top_message .message li          { font-size: 14px;  margin-left: 10px; margin-bottom:2px; background-color: #FF3030; padding:3px;}
#top_message .message li:hover    { color: #ffff00; background-color: #ff0000;}
#top_message .close-message       { background: url(images/jqueryui/modalClose.png) no-repeat; float:right; height:30px; width: 30px; cursor: pointer; margin-right: 5px; margin-top: 3px;}
#top_message .close-message:hover { background: url(images/jqueryui/modalCloseOvr.png);}



