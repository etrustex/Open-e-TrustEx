/*********************************/
/********** PAGE LAYOUT **********/
/*********************************/
html					{ background: #969696; font-size:150px; }
html.ie                 { overflow-x: hidden !important;}
body					{ text-align: left; margin: 0; color: #333;
                          overflow-x: hidden !important;
                          border-left:0; border-right:0; border-bottom: 0;
                          background: #F8F8F8;
                          font-size: 12px;
                          font-weight: normal ;
                          font-style:normal; font-family: 'Helvetica Neue',Arial,Helvetica,sans-serif; }
#main					{ position: relative; clear: both; float: left; width: 100%; overflow:hidden; }
#main-content			{ width: 100%; left: 1%; float:left; position:relative; padding:0; overflow:hidden;}

body.hd                 { border-left:1px solid #888; border-right:1px solid #888; border-bottom:1px solid #888; }

body.mlr-10             { margin-left: 5%; margin-right: 5%;}
body.mlr-20             { margin-left: 10%; margin-right: 10%;}
body.mlr-30,
body     				{ margin-left: 15%; margin-right: 15%;}
body.mlr-0,
body.no-margin			{ margin-left: 0; margin-right: 0;}


/************************************/
/********** COMMON CLASSES **********/
/************************************/

.cl                         { clear: both; }
.ie7 .cl                    { clear: both; zoom:1 !important; line-height:0 !important; height: 0 !important;}

.ie7 .ie7-hidden            { display: none; }

/* CLEAR IS THE ACTIVE DECLARATION, others deprecated : try not to use <div class="cl"></div> anymore, use "clear" on the parent element instead*/
.clear:before, .clear:after { content: ""; display: table; }
.clear:after                { clear: both; }
.ie7 .clear                 { zoom: 1;}

.print-only             	{ display: none !important; }
.hidden     	 			{ display: none !important; }
.display     				{ display: block !important; }
.inline						{ display: inline; }
.inline-block       		{ display: inline-block; }

i.i18n                      { font-style: normal; }

.fl							{ float: left !important}
.fr							{ float: right !important}
.fl-space					{ float: left; margin-right: 5px; }
.fr-space					{ float: right; margin-left: 5px; }
.fl-space2					{ float: left; margin-right: 10px; }
.fr-space2					{ float: right; margin-left: 10px; }
.no-fl                      { float: none !important; }

.center						{ text-align: center !important; margin: auto !important; }
.right						{ text-align: right !important; }
.left						{ text-align: left !important; }
.middle                     { vertical-align: middle !important; }
.top                        { vertical-align: top !important; }
.bottom                     { vertical-align: bottom !important; }

.pos-fixed                  { position: fixed !important;}

.auto-width                 { width: auto !important; padding-left: 2px !important; padding-right: 2px !important; }

.no-margin					{ margin: 0 !important; }
.no-border                  { border: 0 none !important; }
.no-ml                      { margin-left: 0 !important; }
.no-mr                      { margin-right: 0 !important; }
.no-mt                      { margin-top: 0 !important; }
.no-mb                      { margin-bottom: 0 !important; }

.mr                         { margin-right: 10px !important;}
.ml                         { margin-left: 10px !important;}
.mt                         { margin-top: 10px !important;}
.mb                         { margin-bottom: 10px !important;}

.ui-widget-content a        { color: #007abd; text-decoration: none !important; cursor: pointer;}
.bw .ui-widget-content a    { color: #333; }
a							{ color: #007abd; text-decoration: none !important; cursor: pointer;}
.bw a					    { color: #222;}
a.button-secondary-link		{ margin-top: 4px;}
a:hover						{ color: #007abd; text-decoration: underline;}
.bw a:hover			        { color: #000;}

small						{ line-height: 14px; display: block; }

.rule						{ padding-top: 12px  ; padding-bottom: 0; background: url(data:image/gif;base64,R0lGODlhHgACAJEAAAAAAP///8TExAAAACwAAAAAHgACAAACCpSPqWvhD6OcrgAAOw==) 0 10px repeat-x; width: 90%; margin: 8px auto; }
.ie7 .rule                  { background-image: url('images/_base64_sources/rule.gif');}
.hr 						{ clear: both; padding: 0; margin-bottom: 5px; margin-top: 3px; height: 5px; background: transparent url(data:image/gif;base64,R0lGODlhBQAFAJECANbX2P3+/v///wAAACH5BAEAAAIALAAAAAAFAAUAAAIHlA2Zwe1nCgA7) repeat-x 0 0; }
.ie7 .hr                    { background-image: url("images/_base64_sources/hr-bg.gif"); }
.hr.no-margin               { margin:0 !important; }

.cr-help					{ cursor: help; }
.cr-pointer					{ cursor: pointer !important; }
.cr-default                 { cursor: default !important; }

.section-title			                { margin-bottom:10px; margin-top: 15px; width: 100%; border-bottom: 1px solid #dedede; padding-bottom: 5px; }
.section-title.first                    { margin-top: 5px; }
.section-title h1                       { color: #007abd; margin-bottom: 0 !important; padding-bottom: 0 !important;}
.bw .section-title h1                   { color: #333;}
.section-title-bullet                   { margin-bottom:10px; margin-top: 15px; width: auto; border-bottom: 1px solid #dedede; padding: 0 0 5px 12px; background: url(data:image/gif;base64,R0lGODlhEAAQAPQWAFxcXGJiYmVlZWlpaXFxcXp6eoSEhIaGhomJiY6OjpOTk5mZmZ2dnaKioqWlpaampqioqKmpqaurq6+vr7GxsbOzs////6usrQAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAACH5BAEAABcALAAAAAAQABAAAAUo4CWOZGmeaKquY8KwouE0cDFBD0tElCStg8ZCwQocCjCAAMZsOp/MEAA7) no-repeat -3px 3px;}
.section-title-bullet.first             { margin-top: 5px; }
.ie7 .section-title-bullet              { background-image: url(images/_base64_sources/common.bullet_arrow_right.gif);}
.section-title-bullet h1                { color: #007abd;}
.bw .section-title-bullet h1            { color: #333;}
.section-title-bullet.in-section-line   { border-bottom: 1px solid #dedede; width:auto !important; float: left;}
.section-title-bullet .field            { margin-top:-11px; }
.section-title-bullet .button           { margin-top: -6px; margin-right: 0; margin-left: 5px;}


h1 a.label,
h1 a.label:hover,
h2 a.label,
h2 a.label:hover,
h3 span.label,
h4 span.label               { color: #FFFFFF; text-decoration: none; background: #666; display: inline-block; margin-left: 5px; }

h1 a.label					{ padding:0 10px 2px 10px; position: relative; top: -12px; }
h1 a.label:hover			{ background-color: #007abd; }
.bw h1 a.label:hover	    { background-color: #333; }
h2 a.label    				{ margin-left: 5px; padding:0 5px 2px 5px; position: relative; top: -8px; }
h2 a.label:hover			{ background-color: #007abd; }
.bw h2 a.label:hover	    { background-color: #333; }

h3 span.label,
h4 span.label               { margin-left: 5px; position: relative; top: -12px; }

h3 span.label               { padding:0 5px 2px 5px; }
h4 span.label               { padding:0 5px 1px 5px; }

.bw h1 a.label,
.bw h1 a.label:hover,
.bw h2 a.label,
.bw h2 a.label:hover,
.bw h3 span.label,
.bw h4 span.label     { color: #fff; background: #333; }

h1,
h2,
h3,
h4,
h5,
.page-title	    { color: #007abd; }

.bw h1,
.bw h2,
.bw h3,
.bw h4,
.bw h5,
.bw .page-title	{ color: #333; }

h1							{ padding-top: 0; margin-bottom: 1px;  }
h2							{ padding-top: 0; margin-bottom: 10px;  }
h3							{ padding-top: 0; margin-bottom: 8px;  }
h4							{ padding-top: 0; margin-bottom: 5px;  }
h5							{ padding-top: 0; margin-bottom: 1px;  }
h6							{ color: #444; padding-top: 0; margin-bottom: 0; }


#top-reference              { color: #007abd;}

br							{ clear: both; }

.page p						{ margin: 0; padding-bottom: 5px; line-height: 16px; }
.page p.description			{ margin: 0; padding-bottom: 5px; padding-top: 0; line-height: 14px; }

.code						{ background-color: #FAFAFA; color: #007abd; font-family: "Courier New",Courier,monospace,sans-serif;  padding: 2px 5px 3px 5px; margin-bottom: 5px; border-left: 3px solid #dedede; margin-top: 6px;}
.code .highlight 			{ color: #ff001c; }

ul.standard					{ list-style-type: square; padding: 10px 0 10px 16px; }
ul.standard ul				{ list-style-type: square; padding: 5px 0 5px 16px; }

.text-align-right			{ text-align: right !important; }
.text-align-left        	{ text-align: left !important; }
.text-align-center      	{ text-align: center !important; }

.blockUI.blockPage          { border: 1px solid #5A5A5A  !important; }
.blocker-wrapper            { border:1px solid #D1D1D1; }
.blocker-wrapper .loader 	{ background: url(images/common/loading.gif) no-repeat 0 0; margin-left: 46%; width: 40px; height: 40px;}
.blocker                    { color:#0073c4 !important;}
.blocker .subtitle          { color:#009455 !important;}

.highlight-text             { background-color: #1c56ac; color: #fff; padding:0 3px 0 3px;}
.comment-text               { color: #747373; font-size: 11px;}

