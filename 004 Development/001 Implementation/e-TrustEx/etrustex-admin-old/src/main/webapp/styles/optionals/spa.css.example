body{background:#edeef3}#spa-page-container{background-color:#fff}#spa-page{padding:24px}#spa-sidebar-wrapper{background-color:#eee;padding-top:24px;border-right:1px solid #e9e9e9}ul.spa-sidebar{width:100%;color:#999}ul.spa-sidebar li{height:42px;border-bottom:1px solid #dedede;background-color:#fff;cursor:pointer}.ie7 ul.spa-sidebar li{border-bottom:0}ul.spa-sidebar li.selected,ul.spa-sidebar li.selected:hover{background:#5090c8 url(spa/bg-spa-nav-selected.png) no-repeat right}ul.spa-sidebar li:hover{background-color:#eee}ul.spa-sidebar li .icon{color:#a2a2a2;font-size:20px!important;float:left;margin:8px 0 0 16px}ul.spa-sidebar li.selected .icon,ul.spa-sidebar li.selected:hover .icon{color:#fff}ul.spa-sidebar li:hover .icon{color:#5090c8}ul.spa-sidebar li .title{font-size:18px;float:left;margin:8px 0 0 24px}ul.spa-sidebar li:hover .title{color:#666!important}ul.spa-sidebar li.selected .title{color:#fff!important}