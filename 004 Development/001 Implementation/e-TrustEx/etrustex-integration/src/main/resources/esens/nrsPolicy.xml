<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<Policy xmlns="urn:oasis:names:tc:xacml:2.0:policy:schema:os"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      PolicyId="urn:mace:georgetown.edu:sentinel:xacml:policy:xuademo:allow-all"
      RuleCombiningAlgId="urn:oasis:names:tc:xacml:1.0:rule-combining-algorithm:deny-overrides">
    <Description>Policy which allows all access to all patients.</Description>
    <Target/>
    <Rule RuleId="allow-all" Effect="Permit">
        <Description>Any Subject can perform any action on any resource.</Description>
    </Rule>
    <Obligations>        
         <Obligation ObligationId="urn:eSENS:obligations:nro:ETSIREM" FulfillOn="Permit"/>
     </Obligations>    
</Policy>

