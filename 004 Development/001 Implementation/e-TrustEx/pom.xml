				<datasource.name>jdbc/eTrustExDs</datasource.name>
				<transactionmanager.name>org.hibernate.service.jta.platform.internal.WeblogicJtaPlatform</transactionmanager.name>
				<user.password.addAlgorithmPrefix>true</user.password.addAlgorithmPrefix>
				<ear.deployment.dir>${env.WL_DOMAIN_HOME}/autodeploy</ear.deployment.dir>
				<mailSession.name>mail/etrustexMailSession</mailSession.name>
			</properties>
		</profile>		
		<profile>
			<id>JBoss</id>
			<activation>
				<property>
					<name>appserver</name>
					<value>JBOSS</value>
				</property>
			</activation>
