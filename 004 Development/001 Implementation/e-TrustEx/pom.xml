				<saaj.scope>provided</saaj.scope>
				<hibernate.scope>provided</hibernate.scope>
				<jmsConnectionFactory.name>java:/JmsXA</jmsConnectionFactory.name>
				<integrationqueue.name>java:/queue/integrationQueue</integrationqueue.name>
				<waitingroomqueue.name>java:/queue/waitingRoomQueue</waitingroomqueue.name>
				<errorqueue.name>java:/queue/errorQueue</errorqueue.name>
				<sevicequeue.name>java:/queue/serviceQueue</sevicequeue.name>
				<routingqueue.name>java:/queue/routingQueue</routingqueue.name>
				<documentqueue.name>java:/queue/documentQueue</documentqueue.name>
				<datasource.name>java:/jdbc/eTrustExDs</datasource.name>
				<transactionmanager.name>org.hibernate.service.jta.platform.internal.JBossAppServerJtaPlatform</transactionmanager.name>
				<user.password.addAlgorithmPrefix>false</user.password.addAlgorithmPrefix>
				<ear.deployment.dir>${env.JBOSS_HOME}/standalone/deployments</ear.deployment.dir>
				<mailSession.name>java:/mail/etrustexMailSession</mailSession.name>
			</properties>
		</profile>
