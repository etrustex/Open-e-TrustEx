		</xsd:sequence>
	</xsd:complexType>
	<!-- ===== Header Complex Types ===== -->
	<xsd:complexType name="AuthorisationHeaderType">
		<xsd:sequence>
			<xsd:element ref="eccbc:SupplierID"/>
			<xsd:element ref="eccbc:CustomerID"/>
			<xsd:element ref="eccbc:AdditionalID" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ===== e-Catalogue Complex Types ===== -->
	<xsd:complexType name="ItemSynopsisType">
		<xsd:sequence>
			<xsd:element ref="cbc:Name"/>
			<xsd:element ref="cbc:Description" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="cbc:BrandName" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="cbc:ModelName" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="cac:SellersItemIdentification" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ItemClassificationType">
		<xsd:sequence>
			<xsd:element ref="cbc:ItemClassificationCode"/>
			<xsd:element ref="cbc:Description" maxOccurs="unbounded"/>
			<xsd:element ref="ItemClassificationOrderSequence"/>
			<xsd:element ref="ParentItemClassificationCode" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CatalogueItemClassificationType">
		<xsd:sequence>
			<xsd:element ref="ItemClassification"/>
			<xsd:element ref="NumberOfItems"/>
		</xsd:sequence>
	</xsd:complexType>
